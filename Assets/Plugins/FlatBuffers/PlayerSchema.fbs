enum UnitType:byte { Player = 1, Enemy }

table UnitPosition {
	x:float;
	y:float;
}

table UnitFire {
	unitType:UnitType = Player;
	x:float;
	y:float;	
	mouseX:float;
	mouseY:float;
	isSpecial:bool;
}

table UnitDeflect {
	active:bool;
}

table UnitSpawned {
	unitType: UnitType = Player;
	id:int;
	x:float;
	y:float;
}